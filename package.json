    "marked": "^17.0.1",
    "md-editor-v3": "^6.3.0",
    "pinia": "^3.0.4",
    "sass": "^1.97.0",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@prettier/plugin-oxc": "^0.0.5",
    "@tailwindcss/postcss": "^4.1.18",
    "@tsconfig/node24": "^24.0.3",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/eslint-plugin": "^1.5.0",
    "@vue/eslint-config-prettier": "^10.2.0",
